{
  "name": "jsbook",
  "version": "4.0.0",
  "description": "A client-side javascript notebook",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:local": "npm run clean && parcel build src/index.html --public-url / --no-optimize && mkdir dist/examples && cp src/examples/* dist/examples/.",
    "build:github": "npm run clean && parcel build src/index.html --public-url /notebook/ && mkdir dist/examples && cp src/examples/* dist/examples/.",
    "serve": "http-server dist",
    "watch": "parcel watch src/index.html",
    "clean": "rm -rf dist/* && rm -rf ./parcel-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mooreolith/jsbook.git"
  },
  "keywords": [
    "js",
    "notebook"
  ],
  "author": "Joshua M. Moore <mooreolith@gmail.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/mooreolith/jsbook/issues"
  },
  "homepage": "https://github.com/mooreolith/jsbook#readme",
  "dependencies": {
    "@codemirror/commands": "^6.8.0",
    "@codemirror/lang-javascript": "^6.2.3",
    "@codemirror/lang-markdown": "^6.3.2",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.36.4",
    "codemirror": "^6.0.1",
    "marked": "^15.0.7"
  },
  "devDependencies": {
    "process": "^0.11.10"
  }
}
