{
  "manifest_version": 3,
  "short_name": "Notebook",
  "name": "Javascript Notebook",
  "version": "0.1",
  "author": "Joshua M. Moore <joshua.moore@verticesandedges.net",
  "developer": {
    "name": "Joshua M. Moore",
    "url": "https://github.com/mooreolith"
  },
  "homepage_url": "https://apps.verticesandedges.net/notebook/",
  "lang": "en",
  "icons": [
    {
      "src": "./icon/notebook.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "start_url": "https://apps.verticesandedges.net/notebook/",
  "scope": "/notebook/",
  "display": "standalone",
  "background_color": "#ffffff",
  "offline_enabled": true,
  "background": {
    "scripts": ["service-worker.js"],
    "service_worker": ["service-worker.js"]
  },
  "commands": {
    "run-cell": {
      "suggested_key": {
        "default": "Ctrl+Enter"
      },
      "description": "Execute or render the currently focused cell"
    }
  },
  "default_locale": "en",
  "description": "A client-side Javascript Notebook",
  "host_permissions": [
    "storage"
  ]
}
