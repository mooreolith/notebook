{
  "manifest_version": 3,
  "short_name": "Notebook",
  "name": "Javascript Notebook",
  "version": "0.10",
  "author": "Joshua M. Moore <joshua.moore@verticesandedges.net>",
  "developer": {
    "name": "Joshua M. Moore",
    "url": "https://github.com/mooreolith"
  },
  "homepage_url": "https://github.com/mooreolith",
  "lang": "en",
  "icons": [
    {
      "src": "icon/notebook-maskable.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "icon/notebook.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    }
  ],
  "start_url": "https://mooreolith.github.io/notebook/",
  "scope": "/notebook/",
  "display": "standalone",
  "orientation": "any",
  "background_color": "#ffffff",
  "offline_enabled": true,
  "background": {
    "scripts": ["service-worker.js"],
    "service_worker": ["service-worker.js"]
  },
  "commands": {
    "run-cell": {
      "suggested_key": {
        "default": "Ctrl+Enter"
      },
      "description": "Execute or render the currently focused cell"
    }
  },
  "default_locale": "en",
  "description": "A client-side Javascript Notebook",
  "file_handlers": [{
    "action": "/notebook/",
    "accept": {
      "application/x-ipynb+json": [".ipynb", ".ipynb.txt"],
      "text/html": [".ipynb", ".ipynb.txt"]
    }
  }]
}
